[user]
	name = Paul McMonagle
	email = pmcmonagle@tvo.org
[core]
	sharedRepository = group
	editor = /usr/bin/vim
	excludesfile = /Users/pmcmonagle/.gitignore_global
[color]
	ui = true
[push]
	default = matching
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
